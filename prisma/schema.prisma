generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @test.ObjectId
  name      String
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
