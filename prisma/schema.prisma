model Restaurant {
  id        String   @id @default(auto()) @map("_id") @test.ObjectId
  name      String
  location  String
  rating    Float
  details   String
  createdAt DateTime @default(now())
  bookings  Booking[]
}

model Booking {
  id           String   @id @default(auto()) @map("_id") @test.ObjectId
  userId       String
  restaurantId String
  date         DateTime
  createdAt    DateTime @default(now())
  restaurant   Restaurant @relation(fields: [restaurantId], references: [id])
}
